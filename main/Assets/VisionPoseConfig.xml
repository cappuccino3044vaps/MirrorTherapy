<?xml version="1.0" encoding="UTF-8"?>
<Config>
    <!-- Camera Mode 1:Single, 0:Cameraless -->
    <CameraMode>1</CameraMode>

    <!-- Videostream URL -->
    <InputVideoStreamPath><![CDATA[]]></InputVideoStreamPath>

    <!-- Output Skeleton Coordinates CSV 1:Output, 0:Don't output -->
    <SaveSkeletonLog>0</SaveSkeletonLog>
    <!-- Output Skeleton Coordinates Format Version 1 or 2 -->
    <SkeletonLogVersion>2</SkeletonLogVersion>

    <InputSize>
        <!-- Input Color Image Width (unit:pixel) -->
        <Width>640</Width>
        <!-- Input Color Image Height (unit:pixel) -->
        <Height>480</Height>
    </InputSize>

    <!-- Analysis Mode 0: Accuracy-oriented (horizontal) 1:: Speed-oriented (horizontal) 2: Accuracy-oriented (vertical) 3: Speed-oriented (vertical) -->
    <AccuracyMode>0</AccuracyMode>

    <!-- Inference Score Threshold: 0.0-1.0-->
    <ScoreThreshold>0.2</ScoreThreshold>

    <!-- Whether or not to perform 3D analysis 1:Perform, 0:Do not perform -->
    <Enable3DEstimate>1</Enable3DEstimate>

    <!-- Camera Index Settings-->
    <CameraIndex>
        <Main>0</Main>
    </CameraIndex>

    <!-- Color image inverting flag 1: Invert, 0: Do not invert -->
    <EnableMirroring>1</EnableMirroring>

    <!-- Maximum number of people to be estimated -->
    <Single3DInferenceNumber>1</Single3DInferenceNumber>

    <!-- Rotation angle seen from camera perspective 0, 90, -90 -->
    <CameraRotationAngle>0</CameraRotationAngle>

</Config>
